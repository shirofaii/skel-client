{
    "compilerOptions": {
        "module": "amd",
        "moduleResolution": "node",
        "target": "es5",
        "noImplicitAny": false,
        "inlineSources": true,
        "inlineSourceMap": true,
        "watch": true,
        "outDir": "./build/js/",
        "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "lib": [
            "dom",
            "es2015"
        ]
    },
    "compileOnSave": true
}
